{
  "compilerOptions": {
    "target": "es6",
    "strict": true,
    "module": "commonjs",
    "incremental": true,
    "removeComments": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "lib": ["es2019", "dom"],
    "resolveJsonModule": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "declaration": false,
    "noResolve": false,
    "baseUrl": "src/bot",
    "outDir": "../../dest",
    "inlineSourceMap": true
  },
  "include": [
    "./",
    "../../d.ts/types.d.ts",
    "../../d.ts/*"
  ],
  "exclude": [
    "node_modules"
  ]
}