{
  "type": "sqlite",
  "database": "./sogebot.db",
  "logging": ["error"],
  "cache": true,
  "synchronize": false,
  "migrationsRun": true,
  "entities": [
    "dest/database/entity/*.js"
  ],
  "migrations": [
    "dest/database/migration/sqlite/*.js"
  ],
  "subscribers": [
    "dest/database/entity/*.js"
  ],
  "cli": {
    "entitiesDir": "src/bot/database/entity/",
    "migrationsDir": "src/bot/database/migration/sqlite/",
    "subscribersDir": "src/bot/database/entity/"
  }
}