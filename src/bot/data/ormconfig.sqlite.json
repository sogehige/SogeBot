{
  "type": "sqlite",
  "database": "./sogebot.db",
  "logging": ["error"],
  "cache": true,
  "synchronize": false,
  "migrationsRun": true,
  "entities": [
    "dest/database/entity/*.js"
  ],
  "migrations": [
    "dest/database/migration/sqlite/*.js"
  ],
  "subscribers": [
    "dest/database/subscriber/*.js"
  ],
  "cli": {
    "entitiesDir": "src/bot/database/entity/*.ts",
    "migrationsDir": "src/bot/database/migration/sqlite/*.ts",
    "subscribersDir": "src/bot/database/entity/*.ts"
  }
}