  "scripts": {
    "build": "cross-env ENV=development make info eslint bot ui",
    "test": "mocha --recursive --timeout 20000 --exit --require source-map-support/register 2>&1 | tee report && node tools/rerunTests.js",
    "test:config:mongodb": "cp config.example.json config.json && sed -i -- 's/nedb/mongodb/g' config.json",
    "test:config:nedb": "cp config.example.json config.json",
    "migrate": "node ./tools/migrate && node ./tools/migrate --delete",
    "start": "npm run migrate && cross-env NODE_ENV=production node --experimental-worker -r source-map-support/register  ./dest/main || true",
    "start-no-migration": "cross-env NODE_ENV=production node --require source-map-support/register --experimental-worker ./dest/main || true",
    "debug": "cross-env NODE_ENV=development node --require source-map-support/register --inspect  --experimental-worker ./dest/main || true",
    "cleanup": "node ./tools/cleanup.js || true"
  },
