  "scripts": {
    "prestart": "node ./tools/mklogdir.js && echo '... Migration in progress, please wait (see logs/migration.log for progress and error) ...\n' && typeorm migration:run >logs/migration.log 2>&1",
    "watch": "cross-env NODE_ENV=development npx webpack --watch --progress",
    "build": "cross-env ENV=development make bot",
    "test": "node tools/runTests.js",
    "test:config:mysql": "cp src/bot/data/.env.mysql .env",
    "test:config:postgres": "cp src/bot/data/.env.postgres .env",
    "test:config:sqlite": "cp src/bot/data/.env.sqlite .env",
    "migrate": "node ./tools/migrate && node ./tools/migrate --delete",
    "start": "cross-env NODE_ENV=production node --always_compact --optimize_for_size --gc_interval=100 --max_old_space_size=920 -r source-map-support/register  ./dest/main || true",
    "debug": "cross-env NODE_ENV=development node --trace-warnings --always_compact --optimize_for_size --max_old_space_size=920 --gc_interval=100 --require source-map-support/register --inspect=0.0.0.0:9229 --nolazy ./dest/main || true",
    "gc": "cross-env NODE_ENV=development node --trace-warnings --trace_gc --trace_gc_verbose --always_compact --optimize_for_size --max_old_space_size=920 --gc_interval=100 --require source-map-support/register --inspect=0.0.0.0:9229 --nolazy ./dest/main || true",
    "cleanup": "node ./tools/cleanup.js || true"
  },
